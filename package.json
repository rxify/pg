{
  "name": "@rxifyjs/pg",
  "version": "1.2.0",
  "description": "RxJS wrapper for \"pg\"",
  "main": "public-api.js",
  "type": "module",
  "homepage": "https://github.com/rxify/pg#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/rxify/pg.git"
  },
  "bugs": {
    "url": "https://github.com/rxify/pg/issues",
    "email": "alexporrello@gmail.com"
  },
  "bin": {
    "pg-runner": "./cli/bin.js"
  },
  "workspaces": [
    "./tools/build-docs"
  ],
  "scripts": {
    "build": "npx tsc && node tools/postbuild.js",
    "build:watch": "npx tsc --watch",
    "build:prod": "rimraf dist && npm run build",
    "build:docs": "npx typedoc",
    "publish": "cd dist && npm publish --access public",
    "db:start": "npx pg-test start",
    "db:init": "node tools/db-init.js",
    "db:stop": "npx pg-test stop",
    "test": "jest"
  },
  "author": "Alexander Porrello",
  "license": "MIT",
  "dependencies": {
    "pg": "^8.12.0",
    "pg-protocol": "^1.6.1",
    "pg-query-stream": "^4.6.0",
    "rxjs": "^7.8.1",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@databases/pg": "^5.5.0",
    "@databases/pg-test": "^3.1.2",
    "@databases/pg-typed": "^4.4.1",
    "@microsoft/api-documenter": "^7.25.3",
    "@microsoft/api-extractor": "^7.47.0",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.14.2",
    "@types/pg": "^8.11.6",
    "@types/pg-query-stream": "^3.4.0",
    "@types/ping": "^0.4.4",
    "@types/yargs": "^17.0.32",
    "chalk": "^5.3.0",
    "jest": "^29.7.0",
    "ora": "^8.0.1",
    "ping": "^0.4.4",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.25.13",
    "typescript": "^5.4.5"
  }
}